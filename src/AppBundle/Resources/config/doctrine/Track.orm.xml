<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Track" table="track">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idUser" type="integer" column="id_user" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idGenre" type="string" column="id_genre" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="urlPicture" type="string" column="url_picture" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="compositor" type="string" column="compositor" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="explicitContent" type="boolean" column="explicit_content" nullable="true"/>
    <field name="downloadAuthorization" type="string" column="download_authorization" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="transferredAt" type="datetime" column="transferred_at" nullable="true"/>
    <field name="duration" type="time" column="duration" nullable="true"/>
    <field name="nbListenings" type="integer" column="nb_listenings" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nbDownloads" type="integer" column="nb_downloads" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nbViews" type="integer" column="nb_views" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nbLikes" type="integer" column="nb_likes" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
    <field name="nbComments" type="integer" column="nb_comments" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isValidated" type="boolean" column="is_validated" nullable="true"/>
    <many-to-many field="genre" target-entity="Genre" inversed-by="track" fetch="LAZY">
      <join-table name="track_has_genre">
        <join-columns>
          <join-column name="track_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="genre_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="playlist" target-entity="Playlist" inversed-by="track" fetch="LAZY">
      <join-table name="track_has_playlist">
        <join-columns>
          <join-column name="track_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="playlist_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="user" target-entity="User" mapped-by="track" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
